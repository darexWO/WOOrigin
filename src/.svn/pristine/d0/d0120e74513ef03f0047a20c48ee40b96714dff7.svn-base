<%@page contentType="text/html" %>
<%@page pageEncoding="Windows-1250" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<div class="content">
    <c:choose>
        <c:when test="${not empty flashSize}">
            <object width="716px" height="${flashSize}" style="margin:0;padding:0;margin-left:10px;"
                    classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"
                    codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0">
                <param name="SRC" value="/WO/flash/${oj}/podrska.swf"/>
                <param name="scale" value="exactfit"/>
                <embed src="/WO/flash/${oj}/podrska.swf" width="716px" height="${flashSize}"/>
            </object>
        </c:when>
        <c:otherwise>
            <div style="float:left;">
                <div class="navigation_bar_left dark_gray_gradient" style="width:622px;">
                    <h3>PODRŠKA</h3>
                </div>
                <div class="navigation_bar_right" style="width:64px;"><img src="images/strelice.png" alt=""/></div>
            </div>
        </c:otherwise>
    </c:choose>
    <div style="float:left;width:716px;margin-top:25px;height:100%;margin-left:10px;">
        <div style="width:530px;color:#4c4b4b;font-size:12px;line-height:16px;text-align:left;margin:0px auto;">
            <b>Poštovani Partneri,</b><br><br>
            Za sva Vaša pitanja i informacije, na raspolaganju Vam je <b>Darex Partner Podrška.</b><br>
            Svakog radnog dana (od ponedeljka do petka) u periodu <b>od 8:30h do 16:30h.</b><br><br>
            Telefon Darex Partner Podrške: <b>065 31 95 298</b><br>
            E-mail Darex Partner Podrške: <b><a style="color:#f2af26;" href="mailto:webpodrska@darex.rs">webpodrska@darex.rs</a></b>
        </div>
    </div>

</div>
